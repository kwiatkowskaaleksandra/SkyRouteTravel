<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="650.0" prefWidth="1250.0" stylesheets="@../../css/wiadomosc.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Wiadomosci">
   <children>
      <Pane layoutX="-1.0" prefHeight="650.0" prefWidth="687.0">
         <children>
            <TableView fx:id="Tab1" layoutX="1.0" layoutY="64.0" onMouseClicked="#getSelected" prefHeight="583.0" prefWidth="687.0" stylesheets="@../../css/WycieczkiMenu.css">
              <columns>

                <TableColumn fx:id="od" minWidth="0.0" prefWidth="108.0" text="Od:" />
                 <TableColumn fx:id="tem" minWidth="0.0" prefWidth="421.0" text="Temat:" />
                  <TableColumn fx:id="dat" prefWidth="157.0" text="Odebrano:" />
              </columns>
            </TableView>
            <Button fx:id="UsunButton" layoutX="422.0" layoutY="23.0" mnemonicParsing="false" onAction="#UsunWiadomoscOnActionEvent" style="-fx-background-color: #22c4ff;" text="USUN" textFill="WHITE" />
            <Button fx:id="WyjdzButton" layoutX="523.0" layoutY="23.0" mnemonicParsing="false" onAction="#wyjdzButtonOnAction" style="-fx-background-color: #22c4ff;" text="WYJDZ" textFill="WHITE" />
            <TextField fx:id="DanePrac" editable="false" layoutX="14.0" layoutY="23.0" prefHeight="32.0" prefWidth="242.0" />
         </children>
      </Pane>
      <Pane layoutX="686.0" layoutY="64.0" prefHeight="583.0" prefWidth="564.0" style="-fx-background-color: white;">
         <children>
            <Label fx:id="forma" layoutX="14.0" layoutY="21.0" prefHeight="32.0" prefWidth="65.0" text="Od:" textFill="#22c4ff" />
            <Label fx:id="temOdp" layoutX="14.0" layoutY="53.0" prefHeight="25.0" prefWidth="48.0" text="Temat" textFill="#22c4ff" />
            <Label layoutX="14.0" layoutY="91.0" prefHeight="25.0" prefWidth="48.0" text="Treść:" textFill="#22c4ff" />
            <TextArea fx:id="wiad" layoutX="57.0" layoutY="91.0" prefHeight="456.0" prefWidth="493.0" wrapText="true" />
            <TextField fx:id="adres" editable="false" layoutX="57.0" layoutY="25.0" prefHeight="25.0" prefWidth="493.0" />
            <TextField fx:id="subject" editable="false" layoutX="57.0" layoutY="53.0" prefHeight="25.0" prefWidth="493.0" />
         </children></Pane>
      <Button fx:id="WyslijButton" layoutX="1130.0" layoutY="29.0" mnemonicParsing="false" onAction="#WyslijWiadomoscOnActionEvent" style="-fx-background-color: #22c4ff;" text="WYSLIJ" textFill="WHITE" />
      <Button fx:id="OdpowiedzButton" layoutX="1019.0" layoutY="29.0" mnemonicParsing="false" onAction="#OdpowiedzOnActionEvent" style="-fx-background-color: #22c4ff;" text="ODPOWIEDZ" textFill="WHITE" />
   </children>
</AnchorPane>
